{
  "environmentUrl": "https://jhl74831.apps.dynatrace.com",
  "app": {
  "id": "my.d1esa.dynatrace.deepdive",
    "name": "D1 Leadership AI Deep Dive",
  "version": "2.1.1",
    "description": "D1 - AI Deep Dive Presentation App",
    "scopes": [
      {
        "name": "storage:logs:read",
        "comment": "Read access to logs stored in Grail"
      },
      {
        "name": "storage:logs:write",
        "comment": "Write access to ingest logs for admin content management"
      },
      {
        "name": "storage:buckets:read",
        "comment": "Read access to Grail buckets for DQL queries"
      },
      {
        "name": "storage:metrics:read",
        "comment": "Read access to metrics data"
      },
      {
        "name": "storage:entities:read",
        "comment": "Read access to entities"
      },
      {
        "name": "storage:events:read",
        "comment": "Read access to events"
      },
      {
        "name": "app-settings:objects:read",
        "comment": "Read app settings"
      },
      {
        "name": "app-settings:objects:write",
        "comment": "Write app settings"
      }
    ],
  "icon": "src/assets/app_icon.png",
    "selfMonitoringAgent": ""
  },
  "server": {
    "port": 3000,
    "host": "127.0.0.1",
    "showWarnings": true
  },
  "build": {
    "sourceRoot": "."
  }
}
